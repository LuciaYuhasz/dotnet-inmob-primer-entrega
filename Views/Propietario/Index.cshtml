@model IEnumerable<dotnet_inmob_primer_entrega.Models.Propietario>

@{
    ViewData["Title"] = "Listado de Propietarios";
}

<h1>@ViewData["Title"]</h1>

<p>
    <a asp-action="Create" class="btn btn-primary">Crear nuevo</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>DNI</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Tel√©fono</th>
            <th>Email</th>
            <th>Domicilio</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.DNI</td>
                <td>@p.Nombre</td>
                <td>@p.Apellido</td>
                <td>@p.Telefono</td>
                <td>@p.Email</td>
                <td>@p.Domicilio</td>
                <td>
                    <a asp-action="Details" asp-route-id="@p.Id">Ver</a> |
                    <a asp-action="Edit" asp-route-id="@p.Id">Editar</a> |
                    <a asp-action="Delete" asp-route-id="@p.Id">Eliminar</a>
                </td>
            </tr>
        }
    </tbody>
</table>
