@model dotnet_inmob_primer_entrega.Models.Propietario

@{
    ViewData["Title"] = "Detalles del Propietario";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>Propietario</h4>
    <dl class="row">
        <dt class="col-sm-2">DNI</dt>
        <dd class="col-sm-10">@Model.DNI</dd>
        <dt class="col-sm-2">Nombre</dt>
        <dd class="col-sm-10">@Model.Nombre</dd>
        <dt class="col-sm-2">Apellido</dt>
        <dd class="col-sm-10">@Model.Apellido</dd>
        <dt class="col-sm-2">Tel√©fono</dt>
        <dd class="col-sm-10">@Model.Telefono</dd>
        <dt class="col-sm-2">Email</dt>
        <dd class="col-sm-10">@Model.Email</dd>
        <dt class="col-sm-2">Domicilio</dt>
        <dd class="col-sm-10">@Model.Domicilio</dd>
    </dl>
</div>
<a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Editar</a>
<a asp-action="Index" class="btn btn-secondary">Volver</a>
